#include <stdio.h>

int main() {
    int a, b, c;

    printf("--- Digite tres numeros ---\n");
    printf("Primeiro numero: ");
    scanf("%d", &a);
    printf("Segundo numero: ");
    scanf("%d", &b);
    printf("Terceiro numero: ");
    scanf("%d", &c);

    // Verifica a relacao entre (A + B) e C
    if ((a + b) > c) {
        printf("A soma de A + B e maior que C\n");
    } else if ((a + b) < c) {
        printf("A soma de A + B e menor que C\n");
    } else { // Se nao e maior e nem menor, so pode ser igual
        printf("A soma de A + B e igual a C\n");
    }

    return 0;
}
