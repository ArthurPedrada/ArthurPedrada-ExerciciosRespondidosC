#include <stdio.h>

int main() {
    int A, B;

    printf("--- Maior de Dois Numeros ---\n");
    printf("Digite o primeiro numero: ");
    scanf("%d", &A);
    printf("Digite o segundo numero: ");
    scanf("%d", &B);

    if (A > B) {
        printf("O maior numero e: %d\n", A);
    } else if (B > A) {
        printf("O maior numero e: %d\n", B);
    } else {
        printf("Os numeros sao iguais.\n");
    }

    return 0;
}
