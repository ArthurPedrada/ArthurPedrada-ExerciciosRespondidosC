#include <stdio.h>
// Soma e Multiplicação
int main() {
//Declarando as variáveis 
int a,b,c,d;
printf("Digite o valor de a:");
scanf("%d",&a);
printf("Digite o valor de b:");
scanf("%d",&b);
printf("Digite o valor de c:");
scanf("%d",&c);
printf("Digite o valor de d:");
scanf("%d",&d);


printf("Soma de A e B: %d\n", a + b);
printf("Soma de A e C: %d\n", a + c);
printf("Soma de A e D: %d\n", a + d); 
printf("Soma de B e C: %d\n", b + c);
printf("Soma de B e D: %d\n", b + d);
printf("Soma de C e D: %d\n", c + d);

printf("Multiplicação de A e B: %d\n", a * b);
printf("Multiplicação de A e C: %d\n", a *c);
printf("Multiplicação de A e D: %d\n", a * d); 
printf("multiplicação de B e C: %d\n", b * c);
printf("Multiplicação de B e D: %d\n", b * d);
printf("Multiplicação de C e D: %d\n", c * d);

    return 0;
}
