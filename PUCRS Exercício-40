#include <stdio.h>

int main() {
    // Define o tamanho do vetor
    const int TAMANHO = 10;
    int vetor[TAMANHO]; // Declara um vetor de 10 inteiros

    printf("--- Imprime Vetor na Ordem Inversa ---\n");

    // Preenche o vetor com 10 numeros inteiros fornecidos pelo usuario
    printf("Digite 10 numeros inteiros:\n");
    for (int i = 0; i < TAMANHO; i++) {
        printf("Elemento %d: ", i + 1);
        scanf("%d", &vetor[i]);
    }

    // Imprime o vetor na ordem inversa
    printf("\nElementos do vetor na ordem inversa:\n");
    for (int i = TAMANHO - 1; i >= 0; i--) {
        printf("%d ", vetor[i]);
    }
    printf("\n"); // Adiciona uma nova linha no final para melhor formatacao

    return 0;
}

