
#include <stdio.h>
int main() {
    int valor;
    int negativos = 0;
    printf("--- Contagem de Valores Negativos (5 valores) ---\n");

    for (int i = 0; i < 5; i++) {
        printf("Digite o %dÂº valor: ", i + 1);
        scanf("%d", &valor);

        if (valor < 0) {
            negativos++;
        }
    }
    printf("Quantidade de valores negativos: %d\n", negativos);
    return 0;
}
