#include <stdio.h>
//Calculo de uma Caixa
int main() {
//Declarando as variáveis 
double  c,l,h,v;
printf("Digite o comprimento:"); 
scanf("%lf",&c); 
printf("Digite a largura:"); 
scanf("%lf",&l); 
printf("Digite a Altura:"); 
scanf("%lf",&h);
v = c * l * h;
printf("O volume da caixa é: %.2lf\n", v);
    return 0;
}
