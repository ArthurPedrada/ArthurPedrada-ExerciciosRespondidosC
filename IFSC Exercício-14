#include <stdio.h>

int main() {
    int NumA, NumB;

    printf("--- Impressao em Ordem Inversa ---\n");
    printf("Digite o primeiro numero: ");
    scanf("%d", &NumA);
    printf("Digite o segundo numero: ");
    scanf("%d", &NumB);

    printf("Numeros em ordem inversa: %d, %d\n", NumB, NumA);

    return 0;
}
