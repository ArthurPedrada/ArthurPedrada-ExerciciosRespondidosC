#include <stdio.h>

int main() {
	// Variavel para o numero da tabuada
	int numero;

	printf("--- Tabuada de um Numero (de 1 a 10) ---\n");

	// Solicita e le o numero
	printf("Digite um numero inteiro para ver a tabuada: ");
	scanf("%d", &numero);

	printf("\nTabuada do %d:\n", numero);
	// Loop para imprimir a tabuada de 1 a 10
	for (int i = 1; i <= 10; i++) {
		printf("%d x %d = %d\n", numero, i, numero * i);
	}

	return 0;
}

