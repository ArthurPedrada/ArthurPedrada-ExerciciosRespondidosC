
#include <stdio.h>

int main() {
int n, soma = 0;

    // Leitura do número inteiro positivo N
    printf("Digite um número inteiro positivo N: ");
    scanf("%d", &n);

    // Calculando a soma de 1 até N
    for (int i = 1; i <= n; i++) {
        soma += i;
    }

    // Imprimindo o resultado
    printf("A soma de todos os números inteiros de 1 até %d é: %d\n", n, soma);

    return 0;
}
